one\n","          # else:\n","          #   selectionClientId_1 = None  # 리스트가 비어있으면 None으로 설정\n","          #   element['group'] = None\n","\n","        # raw_text\n","          element['raw_text'] = unit['submissionItems'][0]['answer'] if unit['submissionItems'][0]['answer'] is not None else \"\"\n","\n","        # text_newline\n","          element['text_newline'] = unit['submissionItems'][1]['answer'].split(\"\\n\") if unit['submissionItems'][1]['answer'] else []\n","          if not element['text_newline']:\n","            element['text_newline'] = ['text_newline_error']\n","\n","        # text_alignment\n","          if len(unit['submissionItems']) > 2 and unit['submissionItems'][2]['selectionClientIds']:\n","            selectionClientId_4 = unit['submissionItems'][2]['selectionClientIds'][0]\n","            if selectionClientId_4 == \"1000\":\n","              element['text_alignment'] = \"middle\"\n","            elif selectionClientId_4 == \"1001\":\n","              element['text_alignment'] = \"left\"\n","            elif selectionClientId_4 == \"1002\":\n","              element['text_alignment'] = \"right\"\n","          else:\n","            element['text_alignment'] = \"\"\n","\n","        # highlight_text\n","          element['highlight_text'] = unit['submissionItems'][3]['answer'] if unit['submissionItems'][3]['answer'] else \"\"\n","\n","\n","\n","\n","        # text_font_color\n","          try:\n","            if len(unit['submissionItems']) > 4 and unit['submissionItems'][4]['answer']:\n","              font_color = unit['submissionItems'][4]['answer']\n","\n","              if \"RGB\" in font_color:\n","                rgb_values_str = font_color.replace(\"RGB(\", \"\").replace(\")\", \"\")\n","                font_color = [int(c) for c in rgb_values_str.split(\",\")]\n","                if len(font_color) == 4:\n","                  element['font_color'] = font_color\n","                else:\n","                  element['font_color'] = font_color + [1]\n","              else:\n","                element['font_color'] = [\"color error\"]\n","            else:\n","              element['font_color'] = [\"color error\"]\n","          except ValueError as e:\n","              print(f\"오류 발생 파일: {data['source']['fileName']}\")  # 오류 발생 시 파일 이름 출력\n","              error_files.append(data['source']['fileName'])  # error_files 리스트에 파일 이름 추가\n","              raise e  # 오류 다시 발생시켜 traceback 출력\n","\n","\n","\n","        # text_highlight_color\n","          if len(unit['submissionItems']) > 4 and unit['submissionItems'][5]['answer']:\n","            highlight_color = unit['submissionItems'][5]['answer']\n","            if \"RGB\" in highlight_color:\n","              rgb_values_str = highlight_color.replace(\"RGB(\", \"\").replace(\")\", \"\")\n","              highlight_color = [int(c) for c in rgb_values_str.split(\",\")]\n","              if len(highlight_color) == 4:\n","                element['highlight_color'] = highlight_color\n","              else:\n","                element['highlight_color'] = highlight_color+ [1]\n","            else:\n","              element['highlight_color'] = [\"color error\"]\n","\n","          else:\n","            element['highlight_color'] = None\n","\n","            highlight_color = unit['submissionItems'][5]['answer']\n","\n","        # text_is_gradient\n","          # selectionClientId_2 = unit['submissionItems'][7]['selectionClientIds'][0]\n","          element['is_gradient'] = 0\n","\n","\n","\n","\n","\n","\n","\n","          # item 조건 적용\n","        elif unit['classClientId'] == \"item\" and len(unit['submissionItems']) == 0:\n","          element[\"font_color\"] =None\n","          element[\"highlight_color\"] =None\n","          element[\"background_color\"] = None\n","        #   if unit['submissionItems'][0]['selectionClientIds']:\n","        #     selectionClientId_1 = unit['submissionItems'][0]['selectionClientIds'][0]\n","        #     if selectionClientId_1 == \"1000\":\n","        #       element['group'] = None\n","        #     elif selectionClientId_1 == \"1001\":\n","        #       element['group'] = 0           # int 형태로 변경\n","        #     elif selectionClientId_1 == \"1002\":\n","        #       element['group'] = 1           # int 형태로 변경\n","        #     elif selectionClientId_1 == \"1003\":\n","        #       element['group'] = 2           # int 형태로 변경\n","        #     else:\n","        #       element[\"group\"] = None\n","        #   else:\n","        #     selectionClientId_1 = None  # 리스트가 비어있으면 None으로 설정\n","        #     element['group'] = None\n","\n","\n","\n","\n","          # logo 조건 적용\n","        elif unit['classClientId'] == \"logo\" and len(unit['submissionItems']) == 0:\n","          element[\"font_color\"] =None\n","          element[\"highlight_color\"] =None\n","          element[\"background_color\"] = None\n","        #   if unit['submissionItems'][0]['selectionClientIds']:\n","        #     selectionClientId_1 = unit['submissionItems'][0]['selectionClientIds'][0]\n","\n","\n","        #     if selectionClientId_1 == \"1000\":\n","        #       element['group'] = None\n","        #     elif selectionClientId_1 == \"1001\":\n","        #       element['group'] = 0           # int 형태로 변경\n","        #     elif selectionClientId_1 == \"1002\":\n","        #       element['group'] = 1           # int 형태로 변경\n","        #     elif selectionClientId_1 == \"1003\":\n","        #       element['group'] = 2           # int 형태로 변경\n","        #     else:\n","        #       element[\"group\"] = None\n","        #   else:\n","        #     selectionClientId_1 = None  # 리스트가 비어있으면 None으로 설정\n","        #     element['group'] = None\n","\n","\n","        # element 추가\n","        new_data['elements'].append(element)\n","\n","        # element_id 증가\n","        element_id += 1\n","\n","    return new_data\n","\n","file_path = output_file_path\n","\n","# 줄 단위로 JSON 객체 로드\n","output_json_list = []\n","with open(file_path, 'r') as f:\n","    for index, line in enumerate(f):\n","        # 각 줄을 JSON 객체로 변환\n","        input_json = json.loads(line.strip())\n","        # 변환 실행\n","        output_json = convert_json(input_json, index, image_info)\n","        output_json_list.append(output_json)\n","\n","# 변환된 JSON 리스트\n","print(json.dumps(output_json_list, indent=4))\n","\n","# 변환된 JSON 저장\n","deliver_form_json = '/content/output_test.json'\n","\n","with open(deliver_form_json, 'w', encoding='utf-8') as outfile:\n","    json.dump(output_json_list, outfile, ensure_ascii=False, indent=4)\n","\n","print(f\"변환된 JSON 파일 /content/deliver_form_json에 저장\")\n","\n","# 오류 발생 파일 이름 출력\n","print(\"오류 발생 파일 목록:\")\n","for file_name in error_files:\n","    print(file_name)"]},{"cell_type":"markdown","metadata":{"id":"4PWOG8k56sn_"},"source":["\n"]},{"cell_type":"markdown","metadata":{"id":"K3nUFKDuNMv3"},"source":["# 좌표 매칭 잘 되었는지 상품 이미지 시각화 테스트"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":367},"collapsed":true,"id":"o7DkyNM0Zt2e","outputId":"2a462a60-9484-44e4-e860-837aa9b372eb","executionInfo":{"status":"error","timestamp":1731595552342,"user_tz":-540,"elapsed":309,"user":{"displayName":"PM1 Selectstar","userId":"00110280253431425090"}}},"outputs":[{"output_type":"error","ename":"FileNotFoundError","evalue":"[Errno 2] No such file or directory: '/content/drive/MyDrive/PM1팀/이호은/24-ds-127_SKT_AI_Media/Dataset/00_POC_dataset/03_layout_dataset/raw_image_test/IMG_1862.JPG'","traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mFileNotFoundError\u001b[0m                         Traceback (most recent call last)","\u001b[0;32m<ipython-input-34-d26a2990fa7c>\u001b[0m in \u001b[0;36m<cell line: 24>\u001b[0;34m()\u001b[0m\n\u001b[1;32m     22\u001b[0m \u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m     23\u001b[0m \u001b[0;31m# 이미지 열기\u001b[0m\u001b[0