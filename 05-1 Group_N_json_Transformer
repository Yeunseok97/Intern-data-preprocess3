image_files = []\n","extensions = ('*.jpg', '*.JPG', '*.jpeg', '*.png', '*.bmp')\n","for ext in extensions:\n","    image_files.extend(glob.glob(os.path.join(image_folder_path, ext)))\n","\n","# 결과 이미지를 저장할 리스트\n","output_images = []\n","\n","for image_path in image_files:\n","    file_name = os.path.basename(image_path)  # 이미지 파일명 추출\n","    elements = find_elements_for_image(file_name, data)\n","\n","    if elements is None:\n","        print(f\"JSON에서 {file_name}에 해당하는 요소를 찾을 수 없습니다.\")\n","        continue\n","\n","    # 이미지 로드\n","    image = imread_unicode_pil(image_path)\n","    if image is None:\n","        continue\n","\n","    # BGR에서 RGB로 변환 (OpenCV는 기본적으로 BGR 포맷 사용)\n","    image = cv2.cvtColor(image, cv2.COLOR_BGR2RGB)\n","\n","    # 각 element_id에 대해 다른 색상 지정\n","    colors = {element['element_id']: get_random_color() for element in elements}\n","\n","    # bbox 그리기\n","    for element in elements:\n","        bbox = element['bbox']\n","        element_id = element['element_id']\n","        color = colors[element_id]\n","\n","        # bbox 정보 추출 (x_center, y_center, z, width, height)\n","        x_center = bbox[0]\n","        y_center = bbox[1]\n","        width = bbox[3]\n","        height = bbox[4]\n","\n","        # 좌상단 좌표 계산\n","        x1 = int(x_center - width / 2)\n","        y1 = int(y_center - height / 2)\n","\n","        # 우하단 좌표 계산\n","        x2 = int(x_center + width / 2)\n","        y2 = int(y_center + height / 2)\n","\n","        # 사각형 그리기\n","        cv2.rectangle(image, (x1, y1), (x2, y2), color, thickness=2)\n","\n","        # element_id 표시\n","        cv2.putText(image, str(element_id), (x1, y1 - 5), cv2.FONT_HERSHEY_SIMPLEX, 0.5, color, thickness=2)\n","\n","    # 결과 이미지 저장\n","    output_image_path = os.path.join(output_folder_path, f\"bbox_{file_name}\")\n","    output_image_bgr = cv2.cvtColor(image, cv2.COLOR_RGB2BGR)\n","    cv2.imencode('.jpg', output_image_bgr)[1].tofile(output_image_path)\n","    print(f'결과 이미지 저장 완료: {output_image_path}')\n","\n","    # 이미지를 리스트에 추가 (RGB 형식으로)\n","    output_images.append(image)\n","\n","# # 결과 이미지 모두 표시\n","# n_images = len(output_images)\n","# if n_images == 0:\n","#     print(\"출력할 이미지가 없습니다.\")\n","# else:\n","#     fig, axs = plt.subplots(1, n_images, figsize=(15, 10))\n","#     if n_images == 1:\n","#         axs = [axs]\n","#     for i in range(n_images):\n","#         ax = axs[i]\n","#         ax.imshow(output_images[i])\n","#         ax.axis('off')\n","#     plt.show()"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"VzgvbGAM1mPF","colab":{"base_uri":"https://localhost:8080/","height":35},"executionInfo":{"status":"ok","timestamp":1731453031877,"user_tz":-540,"elapsed":478,"user":{"displayName":"PM1 Selectstar","userId":"00110280253431425090"}},"outputId":"5fbf44fb-5c2a-41de-bab8-4e37f1e9e4be"},"outputs":[{"output_type":"execute_result","data":{"text/plain":["'/content'"],"application/vnd.google.colaboratory.intrinsic+json":{"type":"string"}},"metadata":{},"execution_count":56}],"source":["import os\n","import shutil\n","os.getcwd()"]},{"cell_type":"code","source":["# # 상품 이미지 압축파일 저장\n","# import shutil         # ↓여기 이름 바꿔야함.\n","# shutil.make_archive(\"bbox_check_1113\",\"zip\",\"/content/bbox_check_1113\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":35},"id":"sja25axW66Rs","executionInfo":{"status":"ok","timestamp":1731451969692,"user_tz":-540,"elapsed":476,"user":{"displayName":"PM1 Selectstar","userId":"00110280253431425090"}},"outputId":"59b8e749-6e16-4656-ce81-63f943215b19"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["'/content/bbox_check_1113.zip'"],"application/vnd.google.colaboratory.intrinsic+json":{"type":"string"}},"metadata":{},"execution_count":47}]},{"cell_type":"markdown","metadata":{"id":"eUL55lF7Zt9G"},"source":["# element_id 부여한 크롭 이미지 저장\n"]},{"cell_type":"code","execution_count":68,"metadata":{"colab":{"base_uri":"https://localhost:8080/"