을 사용합니다.\n","df.columns = df.iloc[0]\n","df = df[1:].reset_index(drop=True)\n","print(\"컬럼 이름을 설정하고, 헤더 행을 제거했습니다.\")\n","\n","\n","# 컬럼명을 요구되는 형식으로 변경\n","df.columns = [\n","    'type', 'categoryId', 'categoryName', 'pageVersion', 'pusherRequestId',\n","    '할당 받은 시각', '제출 시각', 'userCodeName', '최종 검수자 할당 시각',\n","    '최종 검수 시각', 'validatorCodeNames', 'status', 'colconSubmissionId',\n","    'submissionCode', 'failCount', '파일명', '이미지', '이미지 파일 다운로드 url',\n","    '이미지 너비', '이미지 높이', '제출 파일 다운로드 url', '제출 파일명',\n","    '작성 이미지 너비', '작성 이미지 높이', '제출 이미지 너비', '제출 이미지 높이', '일치 여부'\n","]\n","\n","\n","# '제출 파일 다운로드 url' 에서 URL 리스트 가져오기\n","url_column = '제출 파일 다운로드 url'\n","file_name_column = '파일명'\n","\n","if url_column in df.columns and file_name_column in df.columns:\n","    urls = df[[url_column, file_name_column]].dropna(subset=[url_column, file_name_column])\n","else:\n","    raise ValueError(f\"'{url_column}' 또는 '{file_name_column}' 컬럼을 찾을 수 없습니다.\")\n","\n","# 이미지 다운로드 및 크기 측정\n","for index, row in urls.iterrows():\n","    try:\n","        url = row[url_column]\n","        # 파일명 생성 ('.png' 확장자 처리)\n","        if not str(row[file_name_column]).endswith('.png'):\n","            file_name = f\"{row[file_name_column]}.png\"\n","        else:\n","            file_name = row[file_name_column]\n","        file_path = os.path.join(output_dir, file_name)\n","c:\\Users\\USER\\Downloads\\valid_errors_1118_deliver\n","        # 이미지 다운로드\n","        response = requests.get(url)\n","        response.raise_for_status()  # 요청에 오류가 있는 경우 예외 발생\n","\n","        # 파일 저장\n","        with open(file_path, 'wb') as f:\n","            f.write(response.content)\n","        print(f\"이미지 다운로드 완료: {file_path}\")\n","\n","        # 이미지 크기 측정 및 데이터프레임에 입력\n","        with Image.open(file_path) as img:\n","            width, height = img.size\n","            df.at[index, '제출 이미지 너비'] = width\n","            df.at[index, '제출 이미지 높이'] = height\n","\n","            # 일치 여부 확인\n","            if int(df.at[index, '작성 이미지 너비']) == width and int(df.at[index, '작성 이미지 높이']) == height:\n","                df.at[index, '일치 여부'] = 'T'\n","            else:\n","                df.at[index, '일치 여부'] = 'F'\n","\n","    except Exception as e:\n","        print(f\"이미지 다운로드 실패: {url} - {e}\")\n","\n","# 업데이트된 데이터프레임을 엑셀 파일로 저장\n","df.to_excel(output_file_path, index=False)\n","print(f\"모든 작업 완료 이미지 다운로드 및 엑셀 파일 업데이트 완료. 파일이 저장된 경로: {output_file_path}\")\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":557},"id":"FFQ_wVCxncEm","executionInfo":{"status":"error","timestamp":1731883755173,"user_tz":-540,"elapsed":285,"user":{"displayName":"PM1 Selectstar","userId":"00110280253431425090"}},"outputId":"c42bd6c5-a513-4997-963a-3e4ca6670e1a"},"execution_count":null,"outputs":[{"output_type":"error","ename":"ParserError","evalue":"Error tokenizing data. C error: Expected 16 fields in line 2, saw 24\n","traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mParserError\u001b[0m                               Traceback (most recent call last)","\u001b[0;32m<ipython-input-55-b26bb30baa95>\u001b[0m in \u001b[0;36m<cell line: 24>\u001b[0;34m()\u001b[0m\n\u001b[1;32m     22\u001b[0m \u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m     23\u001b[0m \u001b[0;31m# CSV 파일 읽기\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0;32m---> 24\u001b[0;31m \u001b[0mdf\u001b[0m \u001b[0;34m=\u001b[0m \u001b[0mpd\u001b[0m\u001b[0;34m.\u001b[0m\u001b[0mread_csv\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0mcsv_file_path\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0mencoding\u001b[0m\u001b[0;34m=\u001b[0m\u001b[0;34m'euc-kr'\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0mheader\u001b[0m\u001b[0;34m=\u001b[0m\u001b[0;32mNone\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0msep\u001b[0m\u001b[0;34m=\u001b[0m\u001b[0;34m','\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0m\u001b[1;32m     25\u