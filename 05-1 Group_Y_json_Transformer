or element in item['elements']:\n","            if element['category'] == 'image':\n","                image_found = True\n","                element_id = element['element_id']\n","\n","                # bbox 값 가져오기\n","                center_x, center_y, _, box_width, box_height = element['bbox']\n","\n","                # 좌표 계산\n","                x1 = max(0, int(center_x - box_width / 2))\n","                y1 = max(0, int(center_y - box_height / 2))\n","                x2 = min(image_width, int(center_x + box_width / 2))\n","                y2 = min(image_height, int(center_y + box_height / 2))\n","\n","                # 이미지 크롭\n","                cropped_img = img.crop((x1, y1, x2, y2))\n","\n","                # 새로운 파일 이름 생성\n","                file_name_without_ext = os.path.splitext(file_name)[0]  # 파일 이름에서 확장자 제거\n","                new_file_name = f\"{file_name_without_ext}-{element_id}.png\"\n","                output_path = os.path.join(output_directory, new_file_name)\n","                # new_file_name = f\"{file_name[:-4]}-{element_id}.png\"\n","                # output_path = os.path.join(output_directory, new_file_name)\n","\n","                # 크롭된 이미지 저장\n","                cropped_img.save(output_path)\n","                print(f\"이미지 저장 {output_path}\")\n","\n","        # image 카테고리가 없을 경우 메시지 출력\n","        if not image_found:\n","            print(f\"{file_name}에 image category 없음.\")\n","    else:\n","        print(f\"이미지 파일이 없음: {image_path}\")"]},{"cell_type":"code","source":["from google.colab import drive\n","drive.mount('/content/drive')"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"6c7n2j5Qw6fT","executionInfo":{"status":"ok","timestamp":1731883544770,"user_tz":-540,"elapsed":2279,"user":{"displayName":"PM1 Selectstar","userId":"00110280253431425090"}},"outputId":"95f84abe-15ea-4e87-c347-555eb75b8c74"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Drive already mounted at /cont